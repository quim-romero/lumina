{
  "name": "lumina",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview --port 4173 --strictPort",
    "cy:run": "cypress run --browser chrome --headless",
    "test:e2e": "concurrently -k -s first -n server,tests \"vite\" \"wait-on http://localhost:5173 && npm run cy:run\"",
    "e2e": "npm run test:e2e",
    "lh:report": "lhci autorun",
    "analyze": "ANALYZE=1 vite build && echo 'dist/stats.html ready'"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@hookform/resolvers": "^5.2.1",
    "@stripe/stripe-js": "^7.8.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "framer-motion": "^12.23.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.7.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.0.15",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@axe-core/webdriverjs": "^4.10.2",
    "@eslint/js": "^9.33.0",
    "@lhci/cli": "^0.15.1",
    "@types/axe-core": "^2.0.2",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "cypress": "^14.5.4",
    "cypress-axe": "^1.6.0",
    "eslint": "^9.33.0",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.0",
    "wait-on": "^8.0.4"
  }
}
